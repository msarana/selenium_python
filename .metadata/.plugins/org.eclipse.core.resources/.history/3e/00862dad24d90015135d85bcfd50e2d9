from selenium import webdriver

driver = webdriver.Firefox()
driver.get("http://www.google.com") 
driver.quit()


driver = webdriver.Chrome()
driver.get("https://www.google.com")
driver.quit()


driver = webdriver.Ie()
driver.get("https://www.google.com")
driver.quit()
